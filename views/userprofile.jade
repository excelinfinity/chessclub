  body.hold-transition.skin-blue.sidebar-mini
    .wrapper
      header.main-header
        // Logo
        a.logo(href='index2.html')
          // mini logo for sidebar mini 50x50 pixels
          span.logo-mini
            b W
            | AP
          // logo for regular state and mobile devices
          span.logo-lg
            b CHESS
            | CLUB
        // Header Navbar: style can be found in header.less
        nav.navbar.navbar-static-top
          // Sidebar toggle button
          a.sidebar-toggle(href='#', data-toggle='push-menu', role='button')
            span.sr-only Toggle navigation
                
            
      // Left side column. contains the logo and sidebar
      aside.main-sidebar
        // sidebar: style can be found in sidebar.less
        section.sidebar
          // sidebar menu: : style can be found in sidebar.less
          ul.sidebar-menu(data-widget='tree')
            li.header MAIN NAVIGATION
            li
              a(href='/')
                i.fa.fa-th
                span HOME
            li
              a(href='/tournament')
                i.fa.fa-th
                span Tournaments
            li
              a(href='/user')
                i.fa.fa-th
                span Members
            li
              a(href='/play')
                i.fa.fa-th
                span play
                span.pull-right-container
                  small.label.pull-right.bg-green upcoming

        // /.sidebar
      // Content Wrapper. Contains page content
      .content-wrapper
        // Content Header (Page header)
        section.content-header
          h1
            | User
            small Profile
        // Main content
        section.content
          // Main row
          .row
            // Left col
            section.col-md-3
              // Profile Image
              .box.box-primary
                .box-body.box-profile
                  img.profile-user-img.img-responsive.img-circle(src='img/user4-128x128.jpg', alt='User profile picture')
                  h3.profile-username.text-center Nina Mcintire
                  p.text-muted.text-center Software Engineer
                  ul.list-group.list-group-unbordered
                    li.list-group-item
                      b ELO Rating
                      a.pull-right 1322
                    li.list-group-item
                      b Total Match
                      a.pull-right 54
                    li.list-group-item
                      b Win
                      a.pull-right 31
                    li.list-group-item
                      b Draw
                      a.pull-right 11
                  
                // /.box-body
              // /.box
            .col-lg-9
                .box
                  .box-header
                    h3.box-title Match history
                  // /.box-header
                  .box-body
                    table#example2.table.table-bordered.table-hover
                      thead
                        tr
                          th Tournament
                          th Opponant
                          th Piece type
                          th Result
                          th ELO Before
                          th ELO of opponent
                          th ELO after
                      tbody
                        tr
                          td Oct_Blitz_1
                          td someone
                          td Black
                          td Won
                          td 1200
                          td 1300
                          td 1276
                        
              
                      
